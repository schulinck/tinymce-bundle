<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="stfalcon_tinymce.command.symlink.class">Stfalcon\Bundle\TinymceBundle\Command\SymlinkCommand</parameter>
    </parameters>

    <services>
        <service id="stfalcon_tinymce.command.symlink" class="%stfalcon_tinymce.command.symlink.class%">
            <tag name="console.command" />
        </service>
    </services>
</container>